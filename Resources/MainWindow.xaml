        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="900" Width="1024"
        ResizeMode="NoResize" WindowStyle="None"
        BorderBrush="#0071B7"
        BorderThickness="0.5">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Make sure that all file names are Case Sensitive! -->
                <ResourceDictionary Source="/Resources/TabControl_BluePillCenter.xaml"/>
                <ResourceDictionary Source="/Resources/Button_DarkBlueSquareStyle.xaml"/>
                <ResourceDictionary Source="/Resources/Button_LightBlueRoundStyle.xaml"/>
                <ResourceDictionary Source="/Resources/Button_LightBlueSquareStyle.xaml"/>
                <ResourceDictionary Source="/Resources/Button_NeonBlueRoundStyle.xaml"/>
                <ResourceDictionary Source="/Resources/Checkbox_GreenSliderModernStyle.xaml"/>
                <ResourceDictionary Source="/Resources/CheckBox_DiscretGreenStyle.xaml"/>
                <ResourceDictionary Source="/Resources/ComboBox_SimpleGrayStyle.xaml"/>
                <ResourceDictionary Source="/Resources/ListBox_SimpleBlueStyle.xaml"/>
                <ResourceDictionary Source="/Resources/GroupBox_DarkSquareModernStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <Canvas x:Key="icons_eye" Width="24" Height="24">
                <Path Stretch="Fill" Fill="White" Data="M12,9A3,3 0 0,0 9,12A3,3 0 0,0 12,15A3,3 0 0,0 15,12A3,3 0 0,0 12,9M12,17A5,5 0 0,1 7,12A5,5 0 0,1 12,7A5,5 0 0,1 17,12A5,5 0 0,1 12,17M12,4.5C7,4.5 2.73,7.61 1,12C2.73,16.39 7,19.5 12,19.5C17,19.5 21.27,16.39 23,12C21.27,7.61 17,4.5 12,4.5Z" />
            </Canvas>

            <Canvas x:Key="icons_refresh" Width="24" Height="24">
                <Path Stretch="Fill" Fill="White" Data="M12 4C15.64 4 18.67 6.59 19.35 10.04C21.95 10.22 24 12.36 24 15C24 17.76 21.76 20 19 20H6C2.69 20 0 17.31 0 14C0 10.91 2.34 8.36 5.35 8.04C6.6 5.64 9.11 4 12 4M17 13V9L16 10C15.09 8.79 13.64 8 12 8C9.24 8 7 10.24 7 13C7 15.76 9.24 18 12 18C14.05 18 15.81 16.77 16.58 15H14.24C13.69 15.61 12.89 16 12 16C10.34 16 9 14.66 9 13C9 11.34 10.34 10 12 10C13.09 10 14.04 10.58 14.56 11.44L13 13H17Z" />
            </Canvas>

        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Popup x:Name="HelpPopup" Width="340" IsOpen="False" Placement="Right" VerticalOffset="340">
            <RichTextBox Background="LightBlue" BorderBrush="Azure">
                <FlowDocument x:Name="PopupContent" />
            </RichTextBox>
        </Popup>
        <Popup x:Name="AppSecretPopup" Width="400" IsOpen="False" Placement="Center">
            <Border>
                <Grid Background="#313130" FocusManager.FocusedElement="{Binding ElementName=pwdAppSecret}">
                    <Label Content="Type in the App Secret:" Margin="32,47,0,0" Width="193" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" />
                    <PasswordBox x:Name="pwdAppSecret" Height="24" Margin="37,78,60,98" IsEnabled="True" />
                    <TextBox x:Name="txtAppSecret" Height="24" Margin="37,78,60,98" Visibility="Hidden" />


                    <StackPanel x:Name="spViewAppSecret" Margin="340,78,35,98" VerticalAlignment="Center">
                        <Rectangle Width="20" Height="18" Fill="#f0f8ff">
                            <Rectangle.OpacityMask>
                                <VisualBrush Stretch="Fill" Visual="{DynamicResource icons_eye}"/>
                            </Rectangle.OpacityMask>
                        </Rectangle>
                    </StackPanel>

                    <Label x:Name="lblAppSecretMsg" Width="332" Margin="37,107,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="Red" Height="23" />
                    <Button x:Name="btnAppSecretSubmit" Content="Submit" Margin="274,140,0,0" Width="95" Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Background="Black" Foreground="White" />
                    <Button x:Name="btnAppSecretCancel" Content="Cancel" Margin="32,140,0,0" Width="95" Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Background="Black" Foreground="White" />
                    <Button x:Name="btnPasteClipboard" Content="Paste from Clipboard" Margin="230,42,0,0" Width="139" Height="31" HorizontalAlignment="Left" VerticalAlignment="Top" Background="Black" Foreground="White" />
                </Grid>
            </Border>
        </Popup>

        <Popup x:Name="AppModulePopup" Width="400" IsOpen="False" Placement="Center">
            <Border>
                <Grid Background="#313130" >
                    <Label Content="These modules are missing on this device:" Margin="32,25,0,0" Width="345" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" />
                    <TextBox x:Name="txtAppModuleList" Margin="37,52,0,0" HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="340" IsEnabled="False" FontSize="10" TextWrapping="Wrap"/>
                    <Label x:Name="lblAppModuleMsg" Content="Would you like to install them?" Width="340" Margin="37,86,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="Red" />
                    <Button x:Name="btnAppModuleInstall" Content="Yes" Margin="274,114,0,10" Width="95" Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Background="Black" Foreground="White" />
                    <Button x:Name="btnAppModuleCancel" Content="No" Margin="32,114,0,10" Width="95" Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Background="Black" Foreground="White" />
                </Grid>
            </Border>
        </Popup>

