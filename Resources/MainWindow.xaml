                    <Label Content="AutoPilot Group Tag" Margin="517,84,0,0" Width="172" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Right" />
                    <TextBox x:Name="txtAPProfileGroupTag" Margin="689,86,0,0" Width="303" Height="22" HorizontalAlignment="Left" VerticalAlignment="Top" TextWrapping="NoWrap" />
