                    <Button x:Name="btnDeviceExtensionChange" Content="Update Extension" Margin="877,353,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="115" Height="32" Style="{DynamicResource ResourceKey=ButtonSimpleBlue}"/>

